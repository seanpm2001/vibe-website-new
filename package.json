{
  "name": "vibe-website-new",
  "version": "0.0.0",
  "scripts": {
    "dev": "vitepress dev vibe",
    "build": "vitepress build vibe",
    "serve": "yarn build && vitepress serve vibe",
    "lint": "eslint  src/**/*.{vue,js}"
  },
  "dependencies": {
    "@docsearch/css": "^3.0.0-alpha.41",
    "@docsearch/js": "^3.0.0-alpha.41",
    "@vueuse/core": "^7.3.0",
    "element-plus": "^1.3.0-beta.10",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "vitepress": "^0.20.9",
    "vue": "^3.2.26",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-plugin-vue": "^8.4.0",
    "sass": "^1.49.4",
    "vite": "^2.6.13",
    "vite-plugin-style-import": "^1.4.1",
    "vitepress-for-component": "^0.16.9",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.2",
    "rollup-plugin-visualizer": "^5.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "jsx-quotes": 2,
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "semi": [
        "error",
        "always",
        {
          "omitLastInOneLineBlock": true
        }
      ],
      "indent": [
        "error",
        2
      ],
      "space-before-function-paren": 0,
      "eol-last": 1,
      "eqeqeq": 2,
      "no-undef": 0,
      "no-unused-vars": "off",
      "no-useless-escape": "off",
      "max-len": [
        "error",
        800,
        4,
        {
          "ignoreUrls": true,
          "ignorePattern": "<path.*"
        }
      ],
      "prefer-const": [
        "error",
        {
          "destructuring": "all",
          "ignoreReadBeforeAssign": false
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
